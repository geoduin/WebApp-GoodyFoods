@{
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AvansSurplusMealService</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/AvansSurplusMealService.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/main.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/views/Home.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/views/List.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/views/Detail.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/views/Content.css" asp-append-version="true" />
    <link rel="shortcut icon" href="~/favicon.ico" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg NavBar">
            <div class="container">
                <a class="navbar-brand m-0" href="#">
                    <img class="Logo" src="../images/Logo AvansSurplusMeals PNG Vierkant.png">
                </a>
                <!-- Sidebalk -->
                <!-- Zijbalk kleine schermen -->
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
                        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <!-- Navigatielijst -->
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" asp-action="index" asp-controller="Home">Terug naar home</a>
                        </li>
                    </ul>
                </div>

            </div>
        </nav>
    </header>
    <main>
        @RenderBody()
    </main>
    <footer class="footer-bottom">
        <h3>Footer</h3>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>